{
	"manifest_version": 3,
	"name": "PlebNames",
	"version": "1.0",
	"permissions": [
		"declarativeNetRequest"
	],
	"host_permissions": [
		"*://*.btc/*"
	],
	"web_accessible_resources": [
		{
			"resources": ["./index.html"],
			"extension_ids": []
		}
	],
	"background": {
		"service_worker": "./redirect.js"
	}
}